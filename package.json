{
  "name": "python-auto-importer",
  "displayName": "Python Auto Importer",
  "description": "Automatically fixes missing python imports",
  "publisher": "siddseethepalli",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/siddseethepalli/vscode-python-auto-import"
  },
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:python-auto-importer.autoImportPython",
    "onCommand:python-auto-importer.buildIndex"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "python-auto-importer.autoImportPython",
        "title": "Auto Import Python"
      },
      {
        "command": "python-auto-importer.buildIndex",
        "title": "Rebuild Index"
      }
    ],
    "configuration": [
      {
        "title": "Python Auto Importer",
        "properties": {
          "python-auto-importer.pythonInterpreterPath": {
            "type": "string",
            "description": "A Python interpreter path to run extension operations."
          }
        }
      }
    ],
    "keybindings": [
      {
        "command": "python-auto-importer.autoImportPython",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i",
        "when": "editorTextFocus"
      },
      {
        "command": "python-auto-importer.buildIndex",
        "key": "ctrl+shift+j",
        "mac": "cmd+shift+j",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "ts-loader": "^9.2.5",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "@vscode/test-electron": "^1.6.2"
  }
}
